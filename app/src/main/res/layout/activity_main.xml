<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.todo.datamodule.TestActivity">

    <Button
        android:id="@+id/http"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:onClick="httpTest"
        android:text="网络测试"/>

    <Button
        app:layout_constraintTop_toBottomOf="@id/http"
        android:id="@+id/db"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:onClick="dbTest"
        android:text="数据库测试"/>

    <Button
        app:layout_constraintTop_toBottomOf="@id/db"
        android:id="@+id/cache"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:onClick="cacheTest"
        android:text="缓存测试"/>

</android.support.constraint.ConstraintLayout>
